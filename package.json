{
  "name": "top-github-dependents-by-stars",
  "version": "0.0.2",
  "description": "CLI tool for sorting dependents repositories and packages by stars",
  "packageManager": "pnpm@10.15.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "top-github-dependents-by-stars": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "start": "node dist/index.js",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "github",
    "dependencies",
    "dependents",
    "stars",
    "cli"
  ],
  "author": "Boshen",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/boshen/top-github-dependents-by-stars.git"
  },
  "bugs": {
    "url": "https://github.com/boshen/top-github-dependents-by-stars/issues"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.0",
    "axios": "^1.12.1",
    "chalk": "^5.6.2",
    "cheerio": "^1.1.2",
    "cli-progress": "^3.12.0",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.1",
    "dotenv": "^17.2.2",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^24.3.3",
    "tsdown": "^0.15.1",
    "typescript": "^5.9.2"
  }
}
