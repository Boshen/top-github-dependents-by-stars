{
  "name": "top-github-dependents-by-stars",
  "version": "0.0.0",
  "description": "CLI tool for sorting dependents repositories and packages by stars",
  "type": "module",
  "bin": {
    "top-github-dependents-by-stars": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown && mv dist/index.js dist/index.mjs",
